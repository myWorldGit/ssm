<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lanpangzi.mapper.business2.ColorMapper2">
	<select id="findAllColors" resultType="com.lanpangzi.pojo.Color">
		Select *from color where cid =#{cid};
	</select>
	<select id="findColorById" resultType="com.lanpangzi.pojo.Color">
		Select * from color where cid=#{cid} limit 0,1
	</select>
</mapper>
